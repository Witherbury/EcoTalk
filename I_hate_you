import java.util.concurrent.TimeUnit;
public class I_hate_you
{
    public static String hateresp[] = {"That is not what I wanted to hear." , "I hate you too" , "I think we should try and be more positive..." , "I hate you more" , "Can't say I feel the same"};
    public void tell()throws Exception
    {
        int haterand = (int)(Math.random()*5);
        TimeUnit.SECONDS.sleep(2);
        System.out.println(hateresp[haterand]);
    }
}
